    "@react-native/typescript-config": "0.76.0",
    "@testing-library/react-native": "^12.8.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
